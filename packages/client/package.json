{
  "name": "challenge_client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui-treasury/layout": "^5.0.0",
    "@mui/icons-material": "^5.10.6",
    "@mui/material": "^5.10.8",
    "@mui/styles": "^5.10.9",
    "@mui/x-data-grid": "^5.17.6",
    "@types/axios": "^0.14.0",
    "axios": "^1.1.2",
    "leaflet": "^1.9.2",
    "leaflet-arrowheads": "^1.4.0",
    "leaflet-geometryutil": "^0.10.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-leaflet": "^3.2.5",
    "react-leaflet-arrowheads": "^1.1.0",
    "react-router-dom": "^6.4.2",
    "react-scripts": "5.0.1",
    "styled-components": "^5.3.6",
    "typescript": "^4.8.4",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:etc/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "project": [
        "./tsconfig.json"
      ]
    },
    "plugins": [
      "only-warn",
      "filenames",
      "etc"
    ],
    "rules": {
      "etc/no-commented-out-code": "warn",
      "filenames/match-exported": 2,
      "no-warning-comments": [
        "warn",
        {
          "terms": [
            "todo",
            "fixme",
            "TODO",
            "FIXME"
          ],
          "location": "anywhere"
        }
      ],
      "no-console": [
        "warn"
      ]
    },
    "settings": {
      "react": {
        "pragma": "React",
        "version": "detect"
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:1337/",
  "devDependencies": {
    "@mui/types": "^7.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/leaflet": "^1.8.0",
    "@types/node": "^16.11.64",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "@types/react-leaflet": "^2.8.2",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "eslint": "^8.25.0",
    "eslint-plugin-boundaries": "^2.5.0",
    "eslint-plugin-etc": "^2.0.2",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-github": "^4.3.3",
    "eslint-plugin-only-warn": "^1.0.2",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-unicorn": "^34.0.1"
  }
}
